{
  "name": "@jondotsoy/splitg",
  "version": "0.4.8",
  "module": "src/index.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/esm/*.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./lib/esm/index.js",
      "import": "./lib/esm/index.js",
      "types": "./lib/esm/index.d.ts"
    }
  },
  "scripts": {
    "prepack": "tsc -p tsconfig.esm.json --outDir ./lib/esm",
    "lint": "prettier --check .",
    "fmt": "prettier --write ."
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.6.3",
    "prettier": "^3.3.3"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "split",
    "string",
    "text",
    "utility",
    "helper"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonDotsoy/splitg.git"
  },
  "homepage": "https://jondotsoy.github.io/splitg/"
}